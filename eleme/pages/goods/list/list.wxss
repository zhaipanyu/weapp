.addAddress {
    padding: 30rpx;
    display: flex;
    align-items: center;
    margin-bottom: 20px;
    background-color: #fff;
}

.addAddress-text {
    flex: 1;
}

.addAddress-img {
    width: 60rpx;
    height: 60rpx;
}
.list-title{
    font-size: 14px;
    color: rgb(121, 118, 118);
}
.list-box{
    background-color: #fff;
    padding: 20rpx;
    line-height: 40px;
}
